pipeline {
    agent any 
    stages {
        stage('DAAAAAAAAAAAAG') { 
            steps {
               sh " pwd"
            }
        }
        stage('Cat-as-print screen') { 
            steps {
               sh " cat text3"
               sh "pwd"
               
            }
        }
        stage('MOUUUSE') { 
            steps {
                sh " pwd"
               sh "echo Mouse-----end"
            }
        }
    }
}
